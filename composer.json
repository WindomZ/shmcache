{
    "name": "windomz/shmcache",
    "description": "",
    "type": "library",
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "WindomZ",
            "email": "git.windomz@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"SHMCache\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"SHMCache\\Test\\": "tests"}
    },
    "scripts": {
        "testing": [
            "parallel-lint . --exclude vendor",
            "phpunit"
        ],
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit ./tests"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
